                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                    </children>
                </GridPane>
                <Button fx:id="returnToTableViewButton" mnemonicParsing="false" onAction="#changeScreenButtonPushed" prefHeight="25.0" prefWidth="84.0" text="Return" />
            </children>
        </VBox>
    </children>
</AnchorPane>
